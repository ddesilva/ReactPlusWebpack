{
  "name": "ReactPlusWebpack",
  "version": "1.0.0",
  "author": "Dilshan de Silva 2017",
  "scripts": {
    "watch": "npm run dev",
    "build:webpack": "webpack --progress --colors --config ./webpack.production.config.js",
    "build:babel": "babel --out-dir build src --copy-files",
    "dev": "webpack-dev-server",
    "build": "npm run clean && npm run build:babel && npm run build:webpack",
    "start": "cross-env ADDR=127.0.0.1 PORT=8080 NODE_ENV=production node build/server.js",
    "clean": "rm -rf dist && rm -rf build",
    "postinstall": "npm run build && npm start"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "bootstrap-sass": "^3.3.7",
    "cors": "2.8.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.15.0",
    "eslint-plugin-react": "^6.9.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.3.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "react-addons-perf": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "resolve-url-loader": "^1.6.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-uglify-js-plugin": "^1.1.9"
  },
  "dependencies": {
    "react": "^15.0.1",
    "react-dom": "^15.0.1"
  }
}
